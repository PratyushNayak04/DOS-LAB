echo "Enter a character : "
read ch 
case $ch in 
[a-z]) echo "You have entered a lowercase alphabet" ;;
[A-Z]) echo "You have entered a Uppercase alphabet" ;;
[0-9]) echo "You have entered a digit" ;; 
?) echo "You have entered a special symbol" ;; 
*) echo "You have entered more than one character" ;;
esac
